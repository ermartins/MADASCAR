$(document).ready(function() {

})

$(window).on('resize', function() {
	var win = $(this);
	if (win.height() > 768) {
		$(".cl_Login").css("width", "510px")
	}
})

function resizeScreen() {
	var win = $(this);
	if (win.height() > 768) {
		$(".cl_Login").css("width", "510px")
	} else {
		$(".cl_Login").css("width", "210px")

	}
}